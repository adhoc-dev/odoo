<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <!-- only used for third checks, may be rename it? -->
    <record id="account_payment_method_new_in_checks" model="account.payment.method">
        <field name="name">New Checks</field>
        <field name="code">new_in_checks</field>
        <field name="payment_type">inbound</field>
    </record>

    <record id="account_payment_method_in_checks" model="account.payment.method">
        <field name="name">Checks</field>
        <field name="code">in_checks</field>
        <field name="payment_type">inbound</field>
    </record>

    <!-- only used for issue checks, may be rename it? -->
    <record id="account_payment_method_new_out_checks" model="account.payment.method">
        <field name="name">New Checks</field>
        <field name="code">new_out_checks</field>
        <field name="payment_type">outbound</field>
    </record>

    <record id="account_payment_method_out_checks" model="account.payment.method">
        <field name="name">Checks</field>
        <field name="code">out_checks</field>
        <field name="payment_type">outbound</field>
    </record>

</odoo>
